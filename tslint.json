{
    "extends": ["tslint:latest", "tslint-config-prettier"],
    "rules": {
        "jsx-no-lambda": false,
        "no-submodule-imports": false,
        "only-arrow-functions": true,
        "arrow-return-shorthand": true,
        "no-trailing-whitespace": true,
        "triple-equals": true,
        "switch-default": true,
        "curly": true,
        "no-implicit-dependencies": [true, "dev"]
    }
}
