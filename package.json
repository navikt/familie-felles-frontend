{
  "name": "familie-felles-frontend",
  "private": true,
  "repository": "https://github.com/navikt/familie-felles-frontend.git",
  "license": "MIT",
  "scripts": {
    "ct": "cz",
    "build": "lerna run build",
    "storybook": "storybook dev -p 8000",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint './packages/**/*.{ts,tsx}' --ignore-pattern '**/dist/*'",
    "lint:fix": "eslint './packages/**/*.{ts,tsx}' --ignore-pattern '**/dist/*' --fix",
    "publish:ci": "lerna publish --yes",
    "publish:canary": "lerna publish --canary",
    "publish:canary:ci": "lerna publish --canary --yes",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "./packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@storybook/addon-a11y": "^9.1.0",
    "@storybook/react-webpack5": "^9.1.0",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/storybook-deployer": "^2.8.16",
    "classnames": "^2.5.1",
    "commitizen": "^4.3.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lerna": "^8.2.3",
    "less": "^4.4.0",
    "less-loader": "^12.3.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^9.1.0",
    "style-loader": "^4.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "webpack": "^5.102.0",
    "eslint-plugin-storybook": "^9.1.0",
    "@storybook/addon-docs": "^9.1.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
