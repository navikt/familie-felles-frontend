{
  "name": "familie-felles-frontend",
  "private": true,
  "repository": "https://github.com/navikt/familie-felles-frontend.git",
  "license": "MIT",
  "scripts": {
    "ct": "cz",
    "build": "lerna run build",
    "storybook": "storybook dev -p 8000",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint './packages/**/*.{ts,tsx}' --ignore-pattern '**/dist/*'",
    "lint:fix": "eslint './packages/**/*.{ts,tsx}' --ignore-pattern '**/dist/*' --fix",
    "publish:ci": "lerna publish --yes",
    "publish:canary": "lerna publish --canary",
    "publish:canary:ci": "lerna publish --canary --yes"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "./packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.4.1",
    "@storybook/addon-a11y": "^8.2.8",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-storysource": "^8.2.8",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/blocks": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/react-webpack5": "^8.2.8",
    "@storybook/storybook-deployer": "^2.8.16",
    "babel-loader": "^9.1.3",
    "classnames": "^2.5.1",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc-06d0b89e-20240801",
    "lerna": "^8.1.8",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.2.8",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "webpack": "^5.93.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
