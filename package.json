{
  "name": "familie-felles-frontend",
  "private": true,
  "repository": "https://github.com/navikt/familie-felles-frontend.git",
  "license": "MIT",
  "scripts": {
    "ct": "cz",
    "build": "lerna run build",
    "storybook": "storybook dev -p 8000",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint './packages/**/*.{ts,tsx}'",
    "lint:fix": "eslint './packages/**/*.{ts,tsx}' --fix",
    "publish:ci": "lerna publish --yes",
    "publish:canary": "lerna publish --canary",
    "publish:canary:ci": "lerna publish --canary --yes"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "./packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@storybook/addon-a11y": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-mdx-gfm": "^7.4.6",
    "@storybook/addon-storysource": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-webpack5": "^7.4.6",
    "@storybook/storybook-deployer": "^2.8.16",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^9.1.3",
    "classnames": "^2.3.2",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lerna": "^7.3.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^15.0.0",
    "lodash.throttle": "^4.1.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-less": "^1.1.3",
    "storybook": "^7.4.6",
    "storybook-addon-react-docgen": "^1.2.44",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
