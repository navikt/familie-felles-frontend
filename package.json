{
  "name": "familie-felles-frontend",
  "private": true,
  "repository": "https://github.com/navikt/familie-felles-frontend.git",
  "license": "MIT",
  "scripts": {
    "ct": "cz",
    "build": "lerna run build",
    "storybook": "storybook dev -p 8000",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "eslint './packages/**/*.{ts,tsx}'",
    "lint:fix": "eslint './packages/**/*.{ts,tsx}' --fix",
    "publish:ci": "lerna publish --yes",
    "publish:canary": "lerna publish --canary",
    "publish:canary:ci": "lerna publish --canary --yes"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "./packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@storybook/addon-a11y": "^8.0.8",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-mdx-gfm": "^8.0.8",
    "@storybook/addon-storysource": "^8.0.8",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/blocks": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-webpack5": "^8.0.8",
    "@storybook/storybook-deployer": "^2.8.16",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "babel-loader": "^9.1.3",
    "classnames": "^2.5.1",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.10.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lerna": "^8.1.2",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "storybook": "^8.0.8",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
